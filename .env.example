# Required values
MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# Caching (optional, defaults to false)
NO_CACHE=false

# Application name (optional, defaults to 'just-weather')
APP_NAME=just-weather

# Rate limiting threshold (optional, defaults to 100)
JAIL_THRESHOLD=100

# Logging configuration (optional, defaults to 'info')
# Allowed values: error, warn, info, debug
LOG_LEVEL=info

# Axiom logging integration (optional)
AXIOM_TOKEN=your_axiom_token_here
AXIOM_DATASET=your_dataset_name

# Environment (optional, defaults to 'local')
# Allowed values: local, production
ENVIRONMENT=local 